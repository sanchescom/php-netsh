{
    "name": "sanchescom/php-netsh",
    "description": "PHP layer for netsh windows utility",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Alexander Efimov",
            "email": "sanches.com@mail.ru"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Sanchescom\\Utility\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sanchescom\\Utility\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --coverage-text --colors=never",
        "check-style": "./vendor/bin/phpcs -p --standard=PSR2 ./src",
        "fix-style": "./vendor/bin/phpcbf --standard=PSR2 ./src"
    },
    "require": {
        "php": "^7.2.8",
        "ptlis/shell-command": "^0.16.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.2",
        "squizlabs/php_codesniffer": "^3.4"
    }
}
